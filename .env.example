# API Configuration
REACT_APP_API_URL=
REACT_APP_API_TIMEOUT=10000

# Environment
REACT_APP_ENV=development

# Authentication
REACT_APP_TOKEN_STORAGE_KEY=authToken
REACT_APP_REFRESH_TOKEN_KEY=refreshToken

# Features
REACT_APP_ENABLE_API_LOGGING=true
REACT_APP_ENABLE_RETRY=true
REACT_APP_MAX_RETRIES=3

# File Upload
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf,doc,docx

# Pagination
REACT_APP_DEFAULT_PAGE_SIZE=10
REACT_APP_MAX_PAGE_SIZE=100
